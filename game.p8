-- rules engine

function victoire()
 if (fget(mget(p.x,p.y),1)) and (fget(mget(p.x,p.y),4)) and (fget(mget(p.x,p.y),7)) then
  _update = v_update
  _draw = v_draw
 end
end

function gameover()
 if (fget(mget(p.x,p.y),3)) then
   z = t()
   sfx(1)
  _update = go_update
  _draw = go_draw
 end
end

