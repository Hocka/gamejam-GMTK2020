-- variables globales

function reset()

 p = {
  x = 17, -- 3
  y = 9, -- 12
  t = 1
 }
 
 stock = {}
 
 camx = 0
 
 out = 1
 
 chud = 1
 
 active_checkpoint = {
  x = 3, 
  y = 12, 
 }
 
 _update = g_update
 _draw = g_draw
 
end

function global_init()
 anim_tiles_list = {16,17,18,19,20,21,32,33}
end
-->8
-- main loop 

function _init()
 reset()
 global_init()
end

function g_update()
 update_camera()
 deplacements()
 victoire()
 gameover()
 if (btn(🅾️)) then reset_checkpoint() end
end

function g_draw()
	 cls()
	 camera(camx,0)
	 map(0,0,0,0,40,18)
	 animation()
  spr(5,p.x*8,p.y*8)
  hud()
end
-->8
-- rules engine

function victoire()
 if (fget(mget(p.x,p.y),1)) and (fget(mget(p.x,p.y),4)) and (fget(mget(p.x,p.y),7)) then
  _update = v_update
  _draw = v_draw
 end
end

function gameover()
 if (fget(mget(p.x,p.y),3)) then
  _update = go_update
  _draw = go_draw
 end
end
-->8
-- hud

function hud()
 rectfill(camx,111,camx+127,135,chud)
 for i = 1, #stock do
  if i == p.t then c = 9 else c = 15 end 
 	print(stock[i].s,camx+(i*6)-3,112,c)
 end
 
 if chud == 8 then 
  rect(camx+0,0,camx+127,127,chud)
  rect(camx+1,1,camx+126,126,chud)
 end
end
-->8
-- checkpoint 

function reset_checkpoint()
 p.x = active_checkpoint.x
 p.y = active_checkpoint.y
 p.t = 1
 stock = {}
 update_camera()
 _update = g_update
 _draw = g_draw
end
-->8
-- camera

function update_camera()
 
 if out == 0 and (p.x * 8) - camx > 64 then
  camx = (p.x) * 8 - 8
 end
 
-- exceptions 
 
-- position 3
exceptions_camera(11,01,5)

--  17 09 checkpoint 1
exceptions_camera(17,09,17)

-- end 
if camx > 192 then camx = 192 end

end

function exceptions_camera(x,y,cam)
	if p.x == x and p.y == y then
	 camx = cam * 8
	end
end
-->8
-- controles et collisions

function collision(newx,newy)
	return fget(mget(newx,newy),0)
end
	
function controlable(x,y)
	return fget(mget(x,y),1)
end

function deplacements()

 if (fget(mget(p.x,p.y),4)) then
  stock = {}
  p.t = 1
  active_checkpoint.x = p.x
  active_checkpoint.y = p.y
  
 end

	local temp = {}
	 
	 temp.x = 0
	 temp.y = 0
	 temp.s = "s"
	 
	if controlable(p.x,p.y) then
	 
	 chud = 1
	 out = 0
	 temp_s = "o"
	 
	 if (btnp(⬅️)) then 
	 	temp.x -=1
	 	temp.s = "⬅️"
	 end	
	 if (btnp(➡️)) then 
	 	temp.x +=1
	 	temp.s = "➡️"
	 end	
	 if (btnp(⬆️)) then 
		 temp.y -=1
		 temp.s = "⬆️"
	 end	
	 if (btnp(⬇️)) then 
	 	temp.y +=1 
	 	temp.s = "⬇️"
	 end	
	  
	 if not collision(p.x+temp.x,p.y+temp.y) then
		 
		 if (temp.x == 0 and temp.y == 0 ) then
		 	return
		 else
			 add(stock,temp)
			 p.x = p.x + temp.x
			 p.y = p.y + temp.y
		  end
	 end
	 
	else
	 
	 out = 1
  chud = 8	
	 
	 if t() % 0.5 == 0 then
	 
	 	if (fget(mget(p.x,p.y),5)) then p.y -= 1 
   elseif (fget(mget(p.x,p.y),2)) then p.y += 1 
   elseif (fget(mget(p.x,p.y),6)) then p.x += 1 
   elseif (fget(mget(p.x,p.y),7)) then p.x -= 1 
  
	  else
		  temp.x = p.x + stock[p.t].x
			 temp.y = p.y + stock[p.t].y
			 p.t += 1
			 
			 if p.t > #stock then
			 p.t = 1
		 end
		 
			 if not collision(temp.x,temp.y) then
				 p.x = temp.x
				 p.y = temp.y
		  end 
		 end
		end	
	end
end 


