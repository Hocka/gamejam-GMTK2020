-- animation

function animation()
  
 if t() % 0.5 == 0 then
  
	 for y = 1, 16 do
	  for x = 1,16 do
	   local tx = camx/8+(x-1)
	   local ty = y - 1
	   local tt = mget(tx,ty) 
	  
	   for tile = 1, #anim_tiles_list do  
	    if tt == anim_tiles_list[tile] then 
      if tt % 2 == 0 then
     	 mset(tx,ty,tt + 1)
      else
      	mset(tx,ty,tt - 1)
      end
     end
    end   
   end
  end
 end
end
