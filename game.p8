-- controles et collisions

function collision(newx,newy)
	return fget(mget(newx,newy),0)
end
	
function controlable(x,y)
	return fget(mget(x,y),1)
end

function deplacements()

 if (fget(mget(p.x,p.y),4)) then
  stock = {}
  p.t = 1
  active_checkpoint.x = p.x
  active_checkpoint.y = p.y
 end

	local temp = {}
	 
	 temp.x = 0
	 temp.y = 0
	 temp.s = "s"
	 
	if controlable(p.x,p.y) then
	 
	 chud = 1
	 out = 0
	 temp_s = "o"
	 
	 if (btnp(â¬…ï¸)) then 
	 	temp.x -=1
	 	temp.s = "â¬…ï¸"
	 end	
	 if (btnp(âž¡ï¸)) then 
	 	temp.x +=1
	 	temp.s = "âž¡ï¸"
	 end	
	 if (btnp(â¬†ï¸)) then 
		 temp.y -=1
		 temp.s = "â¬†ï¸"
	 end	
	 if (btnp(â¬‡ï¸)) then 
	 	temp.y +=1 
	 	temp.s = "â¬‡ï¸"
	 end	
	  
	 if not collision(p.x+temp.x,p.y+temp.y) then
		 
		 if (temp.x == 0 and temp.y == 0 ) then
		 	return
		 else
			 add(stock,temp)
			 p.x = p.x + temp.x
			 p.y = p.y + temp.y
		  end
	 end
	 
	else
	 
	 out = 1
  chud = 8	
	 
	 if t() % 0.5 == 0 then
	 
	 	if (fget(mget(p.x,p.y),5)) then p.y -= 1 sfx(4)
   elseif (fget(mget(p.x,p.y),2)) then p.y += 1 sfx(4)
   elseif (fget(mget(p.x,p.y),6)) then p.x += 1 sfx(4)
   elseif (fget(mget(p.x,p.y),7)) then p.x -= 1 sfx(4)
  
	  else
		  temp.x = p.x + stock[p.t].x
			 temp.y = p.y + stock[p.t].y
			 p.t += 1
			 
			 if p.t > #stock then
			 p.t = 1
		 end
		 
			 if not collision(temp.x,temp.y) then
				 p.x = temp.x
				 p.y = temp.y
				 sfx(0)
		  end 
		 end
		end	
	end
end 


