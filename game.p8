-- autres states

function v_update()
 if (btn(🅾️)) then reset() end
end

function v_draw()
 cls(4)
 camera()
 print("★ gg ! ★",45,50,7)
 print("a winner is you",35,70,7)
end

function go_update()
 if (btn(🅾️)) then reset_checkpoint() end
end

function go_draw()
 cls(8)
 camera()
 print("you fell",45,50,7)
 print("press ❎",45,70,7)
end
