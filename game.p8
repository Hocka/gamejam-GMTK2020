  if donuts[donut][1] < (camx + 128) / 8 and
     donuts[donut][1] > (camx) / 8 and
     donuts[donut][3] == 1 then
   spr(11,donuts[donut][1]*8,donuts[donut][2]*8)   
  end 
 end
end


