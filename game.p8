pico-8 cartridge // http://www.pico-8.com
version 27
__lua__
-- gmtk 2020 
-- by hocka

function _init()
 reset()
end

function reset()
 cls(12)
 p = {
  x = 3,
  y = 12,
  t = 1
 }
 
 stock = {}
 
 camx = 0
 
 out = 1
 
 anim_tiles_list = {16,17,18,19,20,21,32,33}
 
 
 _update = g_update
 _draw = g_draw
 
end

function g_update()
 update_camera()
 deplacements()
 dead()
 victoire()
 if (btn(❎)) then reset() end
end
	
function g_draw()
	 cls()
	 camera(camx,0)
	 map(0,0,0,0,40,18)
	 animation()
  spr(5,p.x*8,p.y*8)
  hud()
end

function update_camera()
 
 if out == 0 and (p.x * 8) - camx > 64 then
  camx = (p.x) * 8
 end
 
 if camx > 192 then camx = 192 end
 
 
end


function hud()
 rectfill(camx,111,camx+127,135,1)
 for i = 1, #stock do
  if i == p.t then c = 9 else c = 15 end 
 	print(stock[i].s,camx+(i*6)-5,112,c)
 end
end
	
function collision(newx,newy)
	return fget(mget(newx,newy),0)
end
	
function controlable(x,y)
	return fget(mget(x,y),1)
end
	
function deplacements()

 if (fget(mget(p.x,p.y),4)) then
  stock = {}
  p.t = 1
 end

	local temp = {}
	 
	 temp.x = 0
	 temp.y = 0
	 temp.s = "s"
	 
	if controlable(p.x,p.y) then
	 
	 
	 out = 0
	 temp_s = "o"
	 
	 if (btnp(⬅️)) then 
	 	temp.x -=1
	 	temp.s = "⬅️"
	 end	
	 if (btnp(➡️)) then 
	 	temp.x +=1
	 	temp.s = "➡️"
	 end	
	 if (btnp(⬆️)) then 
		 temp.y -=1
		 temp.s = "⬆️"
	 end	
	 if (btnp(⬇️)) then 
	 	temp.y +=1 
	 	temp.s = "⬇️"
	 end	
	  
	 if not collision(p.x+temp.x,p.y+temp.y) then
		 
		 if (temp.x == 0 and temp.y == 0 ) then
		 	return
		 else
			 add(stock,temp)
			 p.x = p.x + temp.x
			 p.y = p.y + temp.y
		  end
	 end
	 
	else
	 
	 out = 1
	
	 
	 if t() % 0.5 == 0 then
	 
	 	if (fget(mget(p.x,p.y),5)) then p.y -= 1 
   elseif (fget(mget(p.x,p.y),2)) then p.y += 1 
   elseif (fget(mget(p.x,p.y),6)) then p.x += 1 
   elseif (fget(mget(p.x,p.y),7)) then p.x -= 1 
  
	  else
		  temp.x = p.x + stock[p.t].x
			 temp.y = p.y + stock[p.t].y
			 p.t += 1
			 
			 if p.t > #stock then
			 p.t = 1
		 end
		 
			 if not collision(temp.x,temp.y) then
				 p.x = temp.x
				 p.y = temp.y
		  end
		  
   
		  
		end
		end	
			
	end
end 

function victoire()
 if (fget(mget(p.x,p.y),1)) and (fget(mget(p.x,p.y),4)) and (fget(mget(p.x,p.y),7)) then
  _update = v_update
  _draw = v_draw
 end
end

function v_update()
 if (btn(❎)) then reset() end
end

function v_draw()
 cls(4)
 camera()
 print("★ gg ! ★",45,50,7)
 print("a winner is you",35,70,7)
end

function dead()
 if (fget(mget(p.x,p.y),3)) then
  _update = go_update
  _draw = go_draw
 end
end

function go_update()
 if (btn(❎)) then reset() end
end

function go_draw()
 cls(8)
 camera()
 print("you fell",45,50,7)
 print("press ❎",45,70,7)
end

function animation()
  
 if t() % 0.5 == 0 then
  
	 for y = 1, 16 do
	  for x = 1,16 do
	   local tx = camx/8+(x-1)
	   local ty = y - 1
	   local tt = mget(tx,ty) 
	  
	   for tile = 1, #anim_tiles_list do  
	    if tt == anim_tiles_list[tile] then 
      if tt % 2 == 0 then
     	 mset(tx,ty,tt + 1)
      else
      	mset(tx,ty,tt - 1)
      end
     end
    end   
   end
  end
 end
end

